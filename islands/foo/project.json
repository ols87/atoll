{
  "name": "foo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "islands/foo/src",
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nx/vite:test",
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ]
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "ui/islands/foo",
        "main": "islands/foo/src/foo.element.ts",
        "tsConfig": "islands/foo/tsconfig.json",
        "assets": [
          "islands/foo/*.md"
        ],
        "emptyOutDir": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
