{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./"],
  "entryPointStrategy": "expand",
  "exclude": [
    "**/node_modules/**/*",
    "**/*.spec.ts",
    "**/*.stories.ts",
    "**/*.d.ts",
    "**/*index.ts",
    "./jest.config.ts"
  ],
  "tsconfig": "./tsconfig.docs.json",
  "out": "../docs",
  "plugin": ["typedoc-theme-hierarchy"],
  "theme": "hierarchy"
}
